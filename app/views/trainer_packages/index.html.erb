<p id="notice"><%= notice %></p>

<h1>Trainer Packages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trainer_packages.each do |trainer_package| %>
      <tr>
        <td><%= link_to 'Show', trainer_package %></td>
        <td><%= link_to 'Edit', edit_trainer_package_path(trainer_package) %></td>
        <td><%= link_to 'Destroy', trainer_package, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trainer Package', new_trainer_package_path %>
