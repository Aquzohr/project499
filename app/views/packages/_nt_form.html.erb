

<%= bootstrap_form_for(package, :html=> {:id => 'new_nt_package'}) do |form| %>

    <%= form.text_field :name %>
    <%= form.text_field :price %>
    
    <%= form.fields_for :nontrainer_package, NontrainerPackage.new do |f| %>
      <%= f.number_field :freeze_time %>
    <% end %>
    
    <%= form.submit %>

<% end %>

