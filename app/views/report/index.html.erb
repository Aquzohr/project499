<% if current_user.staff.position == "Admin" %>
  <a class="d-block p-3 text-center text-white bg-danger b4b full-width">
    <%= image_tag('logo2.png', size: "80") %> <br>
    REPORT MENU
  </a>
<% else %>
  <a class="d-block p-3 text-center text-white bg-primary b4b full-width">
    <%= image_tag('logo2.png', size: "80") %> <br>
    REPORT MENU
  </a>
<% end %>

<br><br>

<ul class="nav nav-tabs" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" href="#serve" role="tab" data-toggle="tab">Serve</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" href="#receipt" role="tab" data-toggle="tab">Receipt</a>
  </li>
</ul>

<!-- Tab panes -->
<div class="tab-content">

  <div role="tabpanel" class="tab-pane active show" id="serve">
    <%= render 'serve' %>
  </div>

  <div role="tabpanel" class="tab-pane " id="receipt">
    <%= render 'receipt' %>
  </div>

</div>
