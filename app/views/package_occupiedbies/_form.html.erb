<%= bootstrap_form_for(package_occupiedby) do |f| %>

  <div class="row">
    <div class="col-6">
      <%= f.collection_select :trainer_package_id, TrainerPackage.all, :id, :name %>
    </div>
    <div class="col-6">
      <%= f.collection_select :member_id, Member.all, :id, :codeAndFullname %>
    </div>
  </div>

  <div class="row">
    <div class="col-5">
      <%= f.date_field :start_date, min: Date.today %>
    </div>
    <div class="col-5">
      <%= f.date_field :end_date, min: Date.today %>
    </div>
    <div class="col-2">
      <%= f.number_field :balance_session %>
    </div>
  </div>


  <%= f.submit nil, class: "btn btn-primary btn-block" %>

<% end %>
