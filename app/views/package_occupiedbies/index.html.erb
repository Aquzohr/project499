<a class="d-block p-5 text-center text-white bg-primary b4b full-width">TRAINER SERVICE</a>


<br><br>

<div class="row">
  <div class="col col-md-6">
    <h1>List Member for Trainer Service</h1>
  </div>
  <div class="col col-md-6 text-right">
    <%= link_to 'New Trainer Service', new_package_occupiedby_path, class: 'btn btn-info new', :"data-turbolinks" => false  %>
  </div>
</div>

<br><br>

<table  data-toggle="table"
        data-pagination="true"
        >
  <thead>
    <tr>
      <th>Member</th>
      <th>Package</th>
      <th>Balance session</th>
      <th>Start date</th>
      <th>End date</th>
      <th data-width="100px"></th>
      <th data-width="100px"></th>
    </tr>
  </thead>

  <tbody>
    <% @package_occupiedbies.each do |package_occupiedby| %>
      <tr>
        <td><%= package_occupiedby.member.member_code %></td>
        <td><%= package_occupiedby.trainer_package.name %></td>
        <td><%= package_occupiedby.balance_session %></td>
        <td><%= package_occupiedby.start_date %></td>
        <td><%= package_occupiedby.end_date %></td>
        <td><%= link_to 'Edit', edit_package_occupiedby_path(package_occupiedby), class: 'btn btn-warning btn-block' %></td>
        <td><%= link_to 'Destroy', package_occupiedby, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger btn-block' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


