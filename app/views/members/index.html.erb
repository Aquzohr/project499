<div class="row">
  <div class="col-md-6">
    <h1>Members</h1>
  </div>
  <div class="col-md-6 text-right">
    <%= link_to 'New Member', new_member_path, class: 'btn btn-info'  %>
  </div>
</div>


<table id="table" 
       data-toggle="table"
       data-query-params-type="page"
       data-search="true"
       data-pagination="true"
       data-pagination-loop ="false"
       data-side-pagination="server"
       data-url="members.json">
  <thead>
    <tr>
      <th data-field="member_id" data-sortable="true">Member ID</th>
      <th data-field="fullname" >Fullname</th>
      <th data-field="phone" >Phone</th>
      <th data-field="nontrainer_package" >Nontrainer Package</th>
      <th data-field="start_date" >Start Date</th>
      <th data-field="end_date" >End Date</th>
      <th data-field="freeze_count" >Freeze Count</th>
      <th data-field="edit"></th>
      <th data-field="remove"></th>
    </tr>
  </thead>
</table>

